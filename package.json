{
    "name": "blog",
    "version": "1.0.0",
    "description": "{**When you're done, you can delete the content in this README and update the file with details for others getting started with your repository**}",
    "main": "index.js",
    "scripts": {
        "dev": "vitepress dev .",
        "serve": "vitepress serve",
        "build": "vitepress build",
        "lint": "npx eslint .",
        "lint:fix": "npm run lint --fix",
        "prettier": "prettier --write .",
        "prepare": "husky install",
        "commit": "git-cz"
    },
    "lint-staged": {
        "code/**/*.{js,jsx,ts,tsx}": [
            "echo 'Linting started for staged files...'",
            "prettier --write .",
            "echo 'Linting finished for staged files...'"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://gitee.com/wayliuhaha/blog"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "vitepress": "^1.0.0-beta.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "commitizen": "^4.3.0",
        "commitlint": "^19.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "prettier": "^3.0.0",
        "vite": "^4.4.7"
    }
}
